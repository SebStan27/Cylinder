a=0
b=0
c=''

a = int(input('pierwsza libcza: '))
b = int(input('druga libcza: '))
c = input('co chesz zrobic: \n +\n -\n *\n /\n')

def calculon(a,b,c):
  
  if c == '+':
    print (a + b)

  elif c == '-':
    print (a - b)

  elif c == '*':
    print (a * b)

  elif c == '/':
    print (a / b)


calculon(a,b,c)
